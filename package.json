{
  "name": "expo_native_template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "build:native": "rm -rf ./android && rm -rf ./ios && yarn dlx expo prebuild",
    "prepare": "yarn merge-i18n-resources && yarn type-i18n-resources && yarn generate-env-ts",
    "start": "yarn prepare && expo start",
    "start:clean": "yarn prepare && expo start -c",
    "lint": "expo lint --max-warnings 0",
    "clean": "find . -name node_modules -type d -prune | xargs rm -rf",
    "tsc:watch": "tsc -b -v --watch",
    "android": "yarn prepare && expo run:android",
    "android:clean": "yarn watchman:reset && cd android && ./gradlew clean",
    "android:release": "cd android && NODE_ENV=production ./gradlew -p . :app:bundleRelease",
    "ios": "yarn prepare && expo run:ios",
    "ios:pods": "yarn dlx pod-install",
    "web": "yarn prepare && expo start --web",
    "generate-env-ts": "node ./scripts/generate-env-ts",
    "watchman:reset": "watchman watch-del-all && watchman watch-project \"$(pwd)\"",
    "merge-i18n-resources": "i18next-resources-for-ts toc -i ./src/i18n/translations/en -o ./src/i18n/resources/en.ts && i18next-resources-for-ts toc -i ./src/i18n/translations/fr -o ./src/i18n/resources/fr.ts",
    "type-i18n-resources": "i18next-resources-for-ts interface -i ./src/i18n/translations/en -o ./src/i18n/resources/index.d.ts"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "date-fns": "^4.1.0",
    "expo": "~53.0.7",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.5",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.6",
    "expo-linking": "~7.1.4",
    "expo-localization": "^16.1.5",
    "expo-router": "~5.0.5",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "i18next": "^25.1.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.5.1",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "^15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-config-react-native": "^4.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "i18next-resources-for-ts": "^1.6.0",
    "jest": "^29.7.0",
    "tslib": "^2.8.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
